{
  "name": "SQL Server Dev Container",
  "dockerComposeFile": ["./.devcontainer/docker-compose.yml"],
  "service": "sqlserver",
  "workspaceFolder": "/workspace",
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "ms-mssql.mssql"
  ],
  "postCreateCommand": "bash .devcontainer/startup.sh"
}
